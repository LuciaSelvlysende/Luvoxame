[debug]

gdscript/warnings/shadowed_variable=0
gdscript/warnings/incompatible_ternary=0

[display]

window/vsync/vsync_mode=0

[editor]

movie_writer/movie_file="C:/Users/amych_000/Documents/Godot/Projects/minecraft_clone/recordings.avi"
movie_writer/fps=30

[editor_plugins]

enabled=PackedStringArray("res://addons/SignalVisualizer/plugin.cfg", "res://addons/better_vircle/plugin.cfg", "res://addons/godot_resource_groups/plugin.cfg", "res://addons/resources_spreadsheet_view/plugin.cfg", "res://addons/script-ide/plugin.cfg", "res://addons/simple-gui-transitions/plugin.cfg")

[gui]

theme/custom="res://resources/themes/default.tres"

[gui_transitions]

