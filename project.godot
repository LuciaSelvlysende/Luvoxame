config_version=5

[application]

config/name="Luvoxame"
run/main_scene="res://scenes/game/game.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="luvoxame"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://~misc/icon.png"

[autoload]

Menus="*res://gitignore/menus/menu_manager.gd"
Inventories="*res://classes/items/inventory_manager.gd"
Items="*res://classes/items/item_manager.gd"
Blocks="*res://classes/blocks/block_manager.gd"
Resources="*res://classes/mods/resource_manager.gd"
Mods="*res://classes/mods/mod_manager.gd"

[debug]

gdscript/warnings/shadowed_variable=0

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_resource_groups/plugin.cfg", "res://addons/script-ide/plugin.cfg")

[godot_resource_groups]

auto_rebuild=true

[gui]

timers/tooltip_delay_sec=0.2

